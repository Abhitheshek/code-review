{
    "version": 2,
    "projects": {
      "frontend": {
        "src": "frontend",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      "backend": {
        "src": "backend",
        "use": "@vercel/node"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }